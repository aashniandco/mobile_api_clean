       <!-- ✅ START: ADD THIS NEW ROUTE DEFINITION -->
    <!-- This route will be for logged-in customers to set the shipping price for their own cart -->
    <route url="/V1/aashni/carts/mine/set-shipping-price" method="POST">
        <service class="Aashni\MobileApi\Api\SolrInterface" method="setShippingPrice"/>
        <resources>
            <resource ref="self"/> <!-- 'self' resource means only the logged-in customer can use this -->
        </resources>
        <data>
            <!-- Define the parameters the API expects -->
            <parameter type="float" name="shippingPrice" force="true"/>
        </data>
    </route>
    <!-- ✅ END: ADD THIS NEW ROUTE DEFINITION -->


<!-- ✅ START: THIS IS THE CORRECT AND COMPLETE ROUTE DEFINITION -->
<route url="/V1/aashni/place-order" method="POST">
    <service class="Aashni\MobileApi\Api\SolrInterface" method="placeOrderForCustomer"/>
    <resources>
        <resource ref="self"/>
    </resources>
    <data>
        <parameter type="string" name="paymentMethodCode"/>
        <parameter type="mixed" name="billingAddress"/>
        
        <!-- ✅ THIS LINE MUST BE ADDED -->
        <parameter type="string" name="paymentMethodNonce"/>

    </data>
</route>
<!-- ✅ END: THIS IS THE CORRECT AND COMPLETE ROUTE DEFINITION -->


